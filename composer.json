{
  "name": "yogarine/plastic",
  "license": "MIT",
  "description": "This is a fork of Plastic, created by sleimanx2. Plastic is an Elasticsearch ODM and mapper for Laravel. It renders the developer experience more enjoyable while using Elasticsearch by providing a fluent syntax for mapping , querying and storing eloquent models.",
  "keywords": ["elasticsearch","elastic","laravel","eloquent","model","ODM","mapper"],
  "homepage": "https://github.com/Yogarine/plastic",
  "authors": [
    {
      "name": "Sleiman Sleiman",
      "email": "sleiman@outlook.com",
      "role": "Developer"
    },
    {
      "name": "Alwin Garside",
      "email": "alwin@garsi.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "elasticsearch/elasticsearch": ">=5.3",
    "illuminate/container": ">=5.2",
    "illuminate/contracts": ">=5.2",
    "illuminate/console": ">=5.2",
    "illuminate/database": ">=5.2",
    "illuminate/filesystem": ">=5.2",
    "illuminate/http": ">=5.2",
    "illuminate/pagination": ">=5.2",
    "illuminate/support": ">=5.2",
    "ongr/elasticsearch-dsl": ">=5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0",
    "mockery/mockery": "^1.0",
    "phpunit/php-code-coverage": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Sleimanx2\\Plastic\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sleimanx2\\Plastic\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sleimanx2\\Plastic\\PlasticServiceProvider"
      ]
    }
  }
}
